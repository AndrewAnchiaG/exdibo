@model ErrorViewModel
@{
    ViewData["Title"] = "Error";
}


@{
    if (Convert.ToString(ViewData["Code"]) == "404")
    {
        <img src="~/imagenes/warning.png" width="150px"/>
    } else if (Convert.ToString(ViewData["Code"]) == "500")
    {
        <img src="~/imagenes/appserve.png" width="150px"/>
    }else{
        <img src="~/imagenes/cinco_lemosine.jpg" width="150px"/>
    }
}

<h3>C&oacute;digo Error @ViewData["Code"]</h3>

<h1 class="mt-3"> @ViewBag.ErrorMessage</h1>


<a class="btn btn-lg btn-success mt-4" asp-controller="Expediente" asp-action="Index"><i class="fa-solid fa-house"></i> Inicio</a>

<a class="btn btn-lg btn-success mt-4" asp-controller="Expediente" asp-action="Portada"><i class="fa-solid fa-bars"></i> Menú</a>

